<Window x:Class="Documentos.Configuracion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Documentos"
        mc:Ignorable="d"
        Title="Configuracion" Height="450" Width="800" SizeToContent="WidthAndHeight">
    <StackPanel x:Name="contenedor_StackPanel" Orientation="Vertical">
        <DockPanel>
            <TextBlock x:Name="Ancho_TextBlock" DockPanel.Dock="Left" Text="Ancho"/>
            <TextBox x:Name="Ancho_TextBox" />
        </DockPanel>
        <DockPanel>
            <TextBlock x:Name="Alto_TextBlock" DockPanel.Dock="Left" Text="Alto"/>
            <TextBox x:Name="Alto_TextBox" />
        </DockPanel>
    </StackPanel>
</Window>
