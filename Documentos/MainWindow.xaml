<Window x:Class="Documentos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Documentos"
        mc:Ignorable="d"
        Title="Docs" Height="450" Width="800" WindowStyle="ToolWindow"
        SizeToContent="WidthAndHeight" WindowStartupLocation="Manual">
    <StackPanel Orientation="Vertical">
        <Button Name="nuevoDocumento_Button" Content="Nuevo Documento" Margin="10" Click="NuevoDocumento_Button_Click"></Button>
        <Button Name="configuracion_Button" Content="Configuración" Margin="10" Click="Configuracion_Button_Click"></Button>
    </StackPanel>
</Window>
